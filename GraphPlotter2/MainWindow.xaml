<Window x:Class="GraphPlotter2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphPlotter2"
        mc:Ignorable="d"
        Background="WhiteSmoke"
        Title="Graph Plotter" Height="600" Width="1000"
        Icon="/Image/Icon.png"
        Closing="WindowClosing"
        WindowState="Maximized">
    <Grid>
        <Image Name="bufImage"/>
        <Frame Name="screen" NavigationUIVisibility="Hidden" Background="WhiteSmoke" Navigating="Frame_Navigating"/>
    </Grid>
</Window>
